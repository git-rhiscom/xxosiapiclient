<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rhiscom.persistence.inventario.entity.ConversionUnidadMedidaXXOSI" table="XXOSI">
        <id name="idConversion" type="int">
            <column name="ID" precision="10" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="nombreUnidadMedida" type="string">
            <column name="NAME" length="40" not-null="false" />
        </property>
        <property name="cantidadUnidades" type="int">
            <column name="CANTIDAD" precision="10" scale="0" />
        </property>
        <property name="estado" type="char">
            <column name="STATE" length="1" not-null="false" />
        </property>
    </class>
</hibernate-mapping>